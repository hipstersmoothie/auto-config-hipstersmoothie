{
  "name": "auto-config-hipstersmoothie",
  "version": "4.0.0",
  "description": "hipstersmoothie's auto configuration",
  "main": "config.json",
  "author": "Andrew Lisowski <lisowski54@gmail.com>",
  "repository": "hipstersmoothie/auto-config-hipstersmoothie",
  "license": "MIT",
  "keywords": [
    "auto",
    "config"
  ],
  "scripts": {
    "release": "auto shipit"
  },
  "dependencies": {
    "@auto-it/all-contributors": "^11.0.0",
    "@auto-it/first-time-contributor": "^10.2.3",
    "@auto-it/npm": "^10.2.3",
    "@auto-it/pr-body-labels": "^10.2.3",
    "@auto-it/released": "^10.2.3"
  },
  "devDependencies": {
    "auto": "^10.2.3"
  },
  "peerDependencies": {
    "auto": ">= 10.x"
  },
  "auto": {
    "plugins": [
      "npm",
      "pr-body-labels",
      "released",
      "first-time-contributor",
      "all-contributors"
    ],
    "labels": [
      {
        "name": "breaking-minor",
        "description": "Major version zero (0.y.z) is for initial development. Anything MAY change at any time.",
        "changelogTitle": "ðŸ”¨ Breaking Minor Change",
        "releaseType": "minor"
      },
      {
        "name": "dependency-update",
        "changelogTitle": "ðŸ”© Dependency Updates",
        "releaseType": "none"
      }
    ]
  }
}
